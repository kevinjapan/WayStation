<h1>All Todos</h1>

<section>
   <ul class="all_artifacts_list">
      <% @augmented_todos.each do |todo,task| %>
         <li>
            <%= link_to todo.title, todo %>
            <span class="parent_token">         
               <span class="grey_text">task :  </span>
               <%= link_to task.title, task %>
            </span>

            <div>user: <%= todo.users.distinct.pluck(:email_address) %></div>
            <div>creator: <%= todo.creators.distinct.pluck(:email_address) %></div>

            

         </li>
      <% end %>
  </ul>
</section>

